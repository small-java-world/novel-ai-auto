{
  "profiles": {
    "$default": {
      "detect": ["meta[property=\"og:site_name\"][content=\"NovelAI\"]", "#__next"],
      "selectors": {
        "prompt-input": [
          "div[data-slate-editor] [contenteditable=\"true\"]",
          "div[contenteditable=\"true\"]:not([data-negative])",
          "[role=\"textbox\"][contenteditable=\"true\"]",
          "div[contenteditable=\"true\"][role=\"textbox\"]",
          "#__next textarea",
          "[data-testid=\"prompt-input\"] textarea",
          "[data-testid=\"prompt-input\"]",
          "textarea[aria-label*=\"prompt\" i]",
          "textarea[placeholder*=\"prompt\" i]",
          "textarea[name=\"prompt\"]",
          "textarea[id*=\"prompt\" i]",
          ".prompt-input textarea"
        ],
        "negative-input": [
          "div[data-slate-editor] [contenteditable=\"true\"][data-negative=\"true\"]",
          "[data-testid=\"negative-prompt\"] [contenteditable=\"true\"]",
          "#__next textarea[placeholder*=\"negative\" i]",
          "[role=\"textbox\"][contenteditable=\"true\"][data-negative=\"true\"]",
          "div[contenteditable=\"true\"][data-field=\"negative\"]",
          "[data-testid=\"negative-prompt\"] textarea",
          "[data-testid=\"negative-prompt\"]",
          "textarea[aria-label*=\"negative\" i]",
          "textarea[placeholder*=\"negative\" i]",
          "textarea[name=\"negative\"]",
          "textarea[id*=\"negative\" i]",
          ".prompt-negative textarea"
        ],
        "generate-button": [
          "#__next button[type=\"submit\"]",
          "button[title*=\"generate\" i]",
          "button[class*=\"generate\" i]",
          "[data-testid=\"generate-button\"]",
          "button[aria-label*=\"generate\" i]",
          "button[aria-label*=\"生成\" i]",
          "button[type=\"submit\"]",
          ".generate-button"
        ],
        "seed-input": [
          "[data-testid=\"seed-input\"] input",
          "[data-testid=\"seed-input\"]",
          "input[name=\"seed\"]",
          "input[id*=\"seed\" i]",
          "input[aria-label*=\"seed\" i]"
        ],
        "steps-input": [
          "input[type=\"range\"][aria-label*=\"steps\" i]",
          "[data-testid=\"steps-input\"] input",
          "input[name=\"steps\"]",
          "input[id*=\"step\" i]",
          "input[aria-label*=\"steps\" i]"
        ],
        "sampler-select": [
          "input[role=\"combobox\"][aria-label*=\"sampler\" i]",
          "[data-testid=\"sampler-select\"] select",
          "[data-testid=\"sampler-select\"]",
          "select[name=\"sampler\"]",
          "select[id*=\"sampler\" i]",
          "select[aria-label*=\"sampler\" i]"
        ],
        "cfg-scale-input": [
          "input[type=\"range\"][aria-label*=\"cfg\" i]",
          "[data-testid=\"cfg-input\"] input",
          "input[name=\"cfg\"]",
          "input[id*=\"cfg\" i]",
          "input[aria-label*=\"cfg\" i]"
        ],
        "count-input": [
          "[data-testid=\"image-count-input\"] input",
          "input[name=\"imageCount\"]",
          "input[name=\"count\"]",
          "input[id*=\"count\" i]",
          "input[aria-label*=\"count\" i]"
        ],
        "progress-indicator": [
          "#__next [role=\"progressbar\"]",
          "[role=\"progressbar\"]",
          ".progress-bar"
        ],
        "image-gallery": ["#__next .image-gallery", ".image-gallery", ".novelai-gallery"]
      }
    },
    "character-anime": {
      "detect": [".anime-character-ui", "[data-style=\"anime\"]"],
      "selectors": {
        "prompt-input": [
          { "scope": ".anime-character-ui", "selectors": ["textarea", "[contenteditable=true]"] }
        ],
        "negative-input": [{ "scope": ".anime-character-ui", "selectors": [".negative textarea"] }]
      }
    },
    "character-realistic": {
      "detect": [".realistic-character-ui", "[data-style=\"realistic\"]"],
      "selectors": {
        "prompt-input": [
          {
            "scope": ".realistic-character-ui",
            "selectors": ["textarea.prompt", "[contenteditable=true][data-role=prompt]"]
          }
        ],
        "negative-input": [
          {
            "scope": ".realistic-character-ui",
            "selectors": ["textarea.negative", "[data-role=negative]"]
          }
        ],
        "generate-button": [
          {
            "scope": ".realistic-character-ui",
            "selectors": ["button[type=submit]", "button[title*=generate i]"]
          }
        ],
        "cfg-scale-input": [
          "input[type=range][aria-label*=cfg i]",
          "[data-testid=\"cfg-input\"] input"
        ],
        "progress-indicator": ["#__next [role=progressbar]"],
        "image-gallery": ["#__next .image-gallery", ".novelai-gallery"]
      }
    },
    "character-fantasy": {
      "detect": [
        ".fantasy-character-ui",
        "[data-style=\"fantasy\"]",
        "[data-theme*=\"fantasy\" i]"
      ],
      "selectors": {
        "prompt-input": [
          {
            "scope": ".fantasy-character-ui",
            "selectors": ["textarea.fantasy-prompt", "[contenteditable=true][data-role=fantasy]"]
          }
        ],
        "negative-input": [
          {
            "scope": ".fantasy-character-ui",
            "selectors": ["textarea.fantasy-negative", "[data-role=fantasy-negative]"]
          }
        ],
        "generate-button": [
          {
            "scope": ".fantasy-character-ui",
            "selectors": ["button[type=submit]", "button[aria-label*=fantasy i]"]
          }
        ],
        "steps-input": [
          "input[type=range][aria-label*=steps i]",
          "[data-testid=\"steps-input\"] input"
        ],
        "sampler-select": [
          "input[role=combobox][aria-label*=sampler i]",
          "[data-testid=\"sampler-select\"] select"
        ],
        "cfg-scale-input": [
          "input[type=range][aria-label*=cfg i]",
          "[data-testid=\"cfg-input\"] input"
        ],
        "progress-indicator": ["#__next [role=progressbar]", "[role=progressbar]"],
        "image-gallery": ["#__next .image-gallery", ".novelai-gallery"]
      }
    },
    "character-sci-fi": {
      "detect": [".scifi-character-ui", "[data-style=\"scifi\"]", "[data-theme*=\"scifi\" i]"],
      "selectors": {
        "prompt-input": [
          {
            "scope": ".scifi-character-ui",
            "selectors": ["textarea.scifi-prompt", "[contenteditable=true][data-role=scifi]"]
          }
        ],
        "negative-input": [
          {
            "scope": ".scifi-character-ui",
            "selectors": ["textarea.scifi-negative", "[data-role=scifi-negative]"]
          }
        ],
        "generate-button": [
          {
            "scope": ".scifi-character-ui",
            "selectors": ["button[type=submit]", "button[aria-label*=scifi i]"]
          }
        ],
        "steps-input": [
          "input[type=range][aria-label*=steps i]",
          "[data-testid=\"steps-input\"] input"
        ],
        "sampler-select": [
          "input[role=combobox][aria-label*=sampler i]",
          "[data-testid=\"sampler-select\"] select"
        ],
        "cfg-scale-input": [
          "input[type=range][aria-label*=cfg i]",
          "[data-testid=\"cfg-input\"] input"
        ],
        "progress-indicator": ["#__next [role=progressbar]", "[role=progressbar]"],
        "image-gallery": ["#__next .image-gallery", ".novelai-gallery"]
      }
    },
    "character-horror": {
      "detect": [".horror-character-ui", "[data-style=\"horror\"]", "[data-theme*=\"horror\" i]"],
      "selectors": {
        "prompt-input": [
          {
            "scope": ".horror-character-ui",
            "selectors": ["textarea.horror-prompt", "[contenteditable=true][data-role=horror]"]
          }
        ],
        "negative-input": [
          {
            "scope": ".horror-character-ui",
            "selectors": ["textarea.horror-negative", "[data-role=horror-negative]"]
          }
        ],
        "generate-button": [
          {
            "scope": ".horror-character-ui",
            "selectors": ["button[type=submit]", "button[aria-label*=horror i]"]
          }
        ],
        "steps-input": [
          "input[type=range][aria-label*=steps i]",
          "[data-testid=\"steps-input\"] input"
        ],
        "sampler-select": [
          "input[role=combobox][aria-label*=sampler i]",
          "[data-testid=\"sampler-select\"] select"
        ],
        "cfg-scale-input": [
          "input[type=range][aria-label*=cfg i]",
          "[data-testid=\"cfg-input\"] input"
        ],
        "progress-indicator": ["#__next [role=progressbar]", "[role=progressbar]"],
        "image-gallery": ["#__next .image-gallery", ".novelai-gallery"]
      }
    },
    "novelai-mobile": {
      "detect": [".mobile-ui", "[data-view=\"mobile\"]", "@media (max-width: 768px)"],
      "selectors": {
        "prompt-input": [
          ".mobile-prompt textarea",
          "[data-mobile=\"prompt\"] textarea",
          ".mobile-ui textarea[aria-label*=\"prompt\" i]"
        ],
        "negative-input": [
          ".mobile-negative textarea",
          "[data-mobile=\"negative\"] textarea",
          ".mobile-ui textarea[aria-label*=\"negative\" i]"
        ],
        "generate-button": [
          ".mobile-generate-btn",
          "[data-mobile=\"generate\"]",
          ".mobile-ui button[type=\"submit\"]"
        ],
        "progress-indicator": [".mobile-progress", "[data-mobile=\"progress\"]"],
        "image-gallery": [".mobile-gallery", "[data-mobile=\"gallery\"]"]
      }
    },
    "novelai-v3-ui": {
      "detect": [".novelai-v3", "[data-version=\"3\"]", ".v3-interface"],
      "selectors": {
        "prompt-input": [
          ".v3-prompt-area textarea",
          "[data-v3=\"prompt-input\"]",
          ".prompt-v3 [contenteditable=true]",
          "div[data-slate-editor] [contenteditable=true]"
        ],
        "negative-input": [
          ".v3-negative-area textarea",
          "[data-v3=\"negative-input\"]",
          ".negative-v3 [contenteditable=true]"
        ],
        "generate-button": [
          ".v3-generate-button",
          "[data-v3=\"generate\"]",
          "button[aria-label*=\"Generate\" i]"
        ],
        "seed-input": [".v3-seed input", "[data-v3=\"seed\"]", "input[placeholder*=\"seed\" i]"],
        "width-input": [".v3-width input", "[data-v3=\"width\"]", "input[aria-label*=\"width\" i]"],
        "height-input": [
          ".v3-height input",
          "[data-v3=\"height\"]",
          "input[aria-label*=\"height\" i]"
        ],
        "model-select": [
          ".v3-model select",
          "[data-v3=\"model\"]",
          "select[aria-label*=\"model\" i]"
        ],
        "progress-indicator": [
          ".v3-progress",
          "[data-v3=\"progress\"]",
          ".progress-v3 [role=\"progressbar\"]"
        ],
        "image-gallery": [".v3-gallery", "[data-v3=\"gallery\"]", ".gallery-v3 .image-grid"]
      }
    },
    "novelai-nai-diffusion": {
      "detect": [".nai-diffusion", "[data-model=\"nai-diffusion\"]"],
      "selectors": {
        "prompt-input": [
          ".nai-prompt textarea",
          "[data-nai=\"prompt\"]",
          ".diffusion-prompt [contenteditable=true]"
        ],
        "negative-input": [
          ".nai-negative textarea",
          "[data-nai=\"negative\"]",
          ".diffusion-negative [contenteditable=true]"
        ],
        "generate-button": [
          ".nai-generate",
          "[data-nai=\"generate\"]",
          "button[title*=\"diffusion\" i]"
        ],
        "enhance-checkbox": [
          ".nai-enhance input[type=checkbox]",
          "[data-nai=\"enhance\"]",
          "input[aria-label*=\"enhance\" i]"
        ],
        "quality-select": [
          ".nai-quality select",
          "[data-nai=\"quality\"]",
          "select[aria-label*=\"quality\" i]"
        ],
        "progress-indicator": [".nai-progress", "[data-nai=\"progress\"]"],
        "image-gallery": [".nai-gallery", "[data-nai=\"gallery\"]"]
      }
    },
    "novelai-image-2-image": {
      "detect": [".img2img-ui", "[data-mode=\"img2img\"]"],
      "selectors": {
        "prompt-input": [".img2img-prompt textarea", "[data-img2img=\"prompt\"]"],
        "negative-input": [".img2img-negative textarea", "[data-img2img=\"negative\"]"],
        "source-image-upload": [
          ".img2img-upload input[type=file]",
          "[data-img2img=\"upload\"]",
          "input[accept*=\"image\" i]"
        ],
        "strength-input": [
          ".img2img-strength input[type=range]",
          "[data-img2img=\"strength\"]",
          "input[aria-label*=\"strength\" i]"
        ],
        "noise-input": [
          ".img2img-noise input[type=range]",
          "[data-img2img=\"noise\"]",
          "input[aria-label*=\"noise\" i]"
        ],
        "generate-button": [".img2img-generate", "[data-img2img=\"generate\"]"],
        "progress-indicator": [".img2img-progress", "[data-img2img=\"progress\"]"],
        "image-gallery": [".img2img-gallery", "[data-img2img=\"gallery\"]"]
      }
    },
    "novelai-inpainting": {
      "detect": [".inpaint-ui", "[data-mode=\"inpaint\"]"],
      "selectors": {
        "prompt-input": [".inpaint-prompt textarea", "[data-inpaint=\"prompt\"]"],
        "negative-input": [".inpaint-negative textarea", "[data-inpaint=\"negative\"]"],
        "canvas-container": [
          ".inpaint-canvas",
          "[data-inpaint=\"canvas\"]",
          "canvas[data-mode=\"inpaint\"]"
        ],
        "brush-size-input": [
          ".inpaint-brush input[type=range]",
          "[data-inpaint=\"brush-size\"]",
          "input[aria-label*=\"brush\" i]"
        ],
        "mask-opacity-input": [
          ".inpaint-opacity input[type=range]",
          "[data-inpaint=\"opacity\"]",
          "input[aria-label*=\"opacity\" i]"
        ],
        "generate-button": [".inpaint-generate", "[data-inpaint=\"generate\"]"],
        "progress-indicator": [".inpaint-progress", "[data-inpaint=\"progress\"]"],
        "image-gallery": [".inpaint-gallery", "[data-inpaint=\"gallery\"]"]
      }
    }
  }
}
